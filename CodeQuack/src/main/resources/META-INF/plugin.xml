<idea-plugin>
    <id>org.example.CodeQuack</id>
    <name>Rubber Duck Debugger</name>
    <vendor>MNM</vendor>

    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.java</depends>

    <extensions defaultExtensionNs="com.intellij">
        <toolWindow id="CodeQuack"
                    secondary="true"
                    icon="org.example.CodeQuackIcons.PLUGIN_ICON"
                    anchor="right"
                    factoryClass="org.example.CodeQuackWindowFactory"/>
    </extensions>
</idea-plugin>